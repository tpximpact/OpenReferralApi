{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Database": {
    "ConnectionString": "",
    "DatabaseName": "openreferral_prod",
    "ServicesCollection": "services",
    "ColumnsCollection": "columns",
    "ViewsCollection": "views"
  },
  "Validator": {
    "dashboardTestingEnabled": true,
    "dashboardTestingPeriod": 24
  },
  "Security": {
    "AllowedCorsOrigins": [
      "https://openreferraluk.org",
      "https://www.openreferraluk.org",
      "https://api.openreferraluk.org"
    ],
    "ValidateSslCertificates": true
  },
  "RateLimiting": {
    "PermitLimit": 100,
    "Window": 60,
    "QueueLimit": 0
  },
  "OpenTelemetry": {
    "Enabled": true,
    "OtlpEndpoint": ""
  }
}
